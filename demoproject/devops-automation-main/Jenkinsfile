pipeline {
    agent any
    tools{
        maven 'Maven'
        jdk 'JDK'
    }
     stages{
        stage('Initialize'){
            steps{
                sh '''  echo 'PATH = ${M2_HOME}/bin:${PATH}'
                        echo 'M2_HOME = /opt/maven'
                   '''             
            }
        }
     }  
        
    
}
